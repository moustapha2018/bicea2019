{% extends 'template.html.twig' %}
{% block title %}Modification d'une categorie{% endblock %}
{% block body %}


    <div class="container" xmlns="http://www.w3.org/1999/html">
        {% for label, messages in app.flashes %}
            {% for message in messages %}
                <div class="alert alert-{{ label }} mt-10">
                    {{ message }}
                </div>
            {% endfor %}
        {% endfor %}

        <h4><span class="glyphicon glyphicon-edit"></span> Modifier une categorie</h4>
        {{ form(form) }}


    </div>



{% endblock %}